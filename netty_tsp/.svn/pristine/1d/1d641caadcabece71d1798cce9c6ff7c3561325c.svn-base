package msg;

import enums.MsgType;

/**
 * Created by flytome on 2017/9/12.
 */

public class TSPConfigure extends  BaseMsg{
    public String  tsp_host;    //ip 端口配置
    public int  idlingtime;     //怠速判断时间
    public double  idling_speed;//怠速判断值m/s
    public int  swerve;         //方向转弯判断值
    public int  gps_uprate;     //上传gs频率值
    public double  acceleration;//急加速度判断值m/s
    public double  deceleration;//急减速度判断值m/s
    public int  angular_velocity;//急转速度判断值m/s
    public double  instantaneous;//瞬时速度判断值m/s

    public TSPConfigure() {
        super();
        setType(MsgType.TSPCONFIGURE);
    }

    public String getTsp_host() {
        return tsp_host;
    }

    public void setTsp_host(String tsp_host) {
        this.tsp_host = tsp_host;
    }

    public int getIdlingtime() {
        return idlingtime;
    }

    public void setIdlingtime(int idlingtime) {
        this.idlingtime = idlingtime;
    }

    public double getIdling_speed() {
        return idling_speed;
    }

    public void setIdling_speed(double idling_speed) {
        this.idling_speed = idling_speed;
    }

    public int getSwerve() {
        return swerve;
    }

    public void setSwerve(int swerve) {
        this.swerve = swerve;
    }

    public int getGps_uprate() {
        return gps_uprate;
    }

    public void setGps_uprate(int gps_uprate) {
        this.gps_uprate = gps_uprate;
    }

    public double getAcceleration() {
        return acceleration;
    }

    public void setAcceleration(double acceleration) {
        this.acceleration = acceleration;
    }

    public double getDeceleration() {
        return deceleration;
    }

    public void setDeceleration(double deceleration) {
        this.deceleration = deceleration;
    }

    public int getAngular_velocity() {
        return angular_velocity;
    }

    public void setAngular_velocity(int angular_velocity) {
        this.angular_velocity = angular_velocity;
    }

    public double getInstantaneous() {
        return instantaneous;
    }

    public void setInstantaneous(double instantaneous) {
        this.instantaneous = instantaneous;
    }
}
